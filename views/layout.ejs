<!DOCTYPE html>
<html>

  <%- include('partials/head') %>
  <body>
  <!--
  nav bar
  //-->
  <%- include('partials/navbar') %>
  <!--

  page body

  //-->

	<div class="page-header">
			<h3 style="text-align:center"> Mckenzie Rockwell's web portfolio</h3>
			<h3 class="centered"><%= locals.title %></h3>
	</div>

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js">
	</script>
	<script src="/bootstrap/js/bootstrap.min.js">
	 </script>
    <div class='container'>
  		<%- body %>
    </div>
  	<script>


  
  	var navbar = $('#navlist');

  	if (matchMedia)
  	{
  		var mq = window.matchMedia("(max-width: 575px)"); 
  		mq.addListener(isAsShorter);
  		isAsShorter(mq);

  		var mq1 = window.matchMedia("(min-width: 576px");
  		mq1.addListener(isWider);
  		isWider(mq1);
  	}

  

  	function isAsShorter(mq){
  		console.log(mq);
  		if(mq.matches){
  			console.log(mq); 

  			navbar.removeClass("nav-tabs"); 

  		}
  	}

  	function isWider(mq1){

  		if(mq1.matches){
  			
  			console.log(mq1);

  			navbar.addClass("nav-tabs"); 

  		}
  	}
  




  	</script>

  </body>
</html>

