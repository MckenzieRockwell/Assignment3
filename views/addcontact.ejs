<div class='row'>
<form class='form-horizontal' method='post' action='/contacts/addcontact'>
	<div class='form-group'>
		<label class='col-sm-4 control-label' for='contactname'>Contact name</label>
		<div class='col-sm-4'>
			<input type='text' class='form-control' id='contactname' name='name'>
		</div>
	</div>
	<div class='form-group'>
		<label class='col-sm-4 control-label' for='contactnum'>Contact number</label>
		<div class='col-sm-4'>
			<input type='text' class='form-control' id='contactnum' name='number'>
		</div>
	</div>
	<div class='form-group'>
		<label class='col-sm-4 control-label' for='contactemail'>Contact email address</label>
		<div class='col-sm-4'>
			<input type='text' class='form-control' id='contactemail' name='mail'>
		</div>
	</div>
	<div class='form-group'>
		<div class='col-sm-2 col-sm-offset-4'>
		<button type='submit' class='btn btn-default'>Add</button
		</div>
	</div>
</form>
</div> 